/* footer */
.footer{
  text-align: center;
  padding:40rpx 0;
}
.footer-link{
  color:#4284F4;
  font-size:32rpx;
  margin-bottom:8rpx;
}
.footer-copyright{
  color:#999;
  font-size:28rpx;
}
/* footer end */
/* pagelist start */
.page-list{
  display: flex;
  padding:20rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom:1px dashed #e5e5e5;
}
.page-list .list-image{
  width: 35%;
  height: 180rpx;
  overflow: hidden;
  
}
.page-list .list-image image{
  width:100%;
}
.page-list .list-box{
  width: 65%;
  padding-left:20rpx;
}
.page-list .list-title{
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  text-overflow:ellipsis; 
  color:#342619;
  padding:4rpx 0;
  font-size:32rpx;
  line-height:48rpx; 
}
.page-list .list-icon{
  margin-top:20rpx;
}
.page-list .list-icon text{
  color:#999;
  margin-right:40rpx;
  font-size:28rpx;
}
/* pagelist end */
.list-hover{
  background-color: #eaeaea;
}
